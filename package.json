{
  "name": "react-typescript-vite-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow npm",
    "lint-staged": "lint-staged",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "deploy-storybook": "NODE_OPTIONS=--openssl-legacy-provider gh-pages -d storybook-static",
    "type-check": "tsc --noEmit",
    "check": "run-p --aggregate-output -l type-check lint fmt:check",
    "fix": "run-s  fmt:fix lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier .",
    "fmt:check": "prettier -c .",
    "fmt:fix": "prettier -w .",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@tauri-apps/cli": "^1.0.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.6.3",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "engines": {
    "node": "18.x"
  },
  "jest": {
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier -w",
      "tsc --noEmit",
      "eslint --fix"
    ],
    "src/**/*.{js,jsx}": [
      "prettier -w",
      "eslint --fix"
    ],
    "vite.config.ts": [
      "prettier -w",
      "tsc --noEmit",
      "eslint --fix"
    ],
    "jest.config.ts": [
      "prettier -w",
      "tsc --noEmit",
      "eslint --fix"
    ],
    ".storybook/**/*.js": [
      "prettier -w",
      "eslint --fix"
    ],
    "index.html": [
      "prettier -w"
    ],
    ".eslintrc.yaml": [
      "prettier -w"
    ],
    ".prettierrc.yaml": [
      "prettier -w"
    ],
    "tsconfig.json": [
      "prettier -w"
    ],
    "tsconfig.node.json": [
      "prettier -w"
    ],
    "package.json": [
      "prettier -w"
    ],
    "package-lock.json": [
      "prettier -w"
    ]
  }
}
